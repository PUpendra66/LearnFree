{
  "version": 2,
  "builds": [
    {
      "src": "coursespage/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "coursespage/wsgi.py"
    }
  ],
  "buildCommand": "pip install pymysql"
}
